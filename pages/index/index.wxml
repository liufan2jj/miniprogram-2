<!--index.wxml-->
<view class="warp">
  <button bindtap="getMaterialList">获取列表</button>
  <button bindtap="loginOut">退出登录</button>
  <van-circle value="{{ value }}" text="{{text}}" />
  <van-count-down use-slot time="{{ time }}" bind:change="changeValue" data-hi="Weixin">
    <text class="item">{{ timeData.hours }}</text>
    <text class="item">{{ timeData.minutes }}</text>
    <text class="item">{{ timeData.seconds }}</text>
  </van-count-down>
</view>